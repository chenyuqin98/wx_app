/* pages/register/register.wxss */

.ImageBox image{
  width: 200rpx;
  height: 200rpx;
  margin-top: 120rpx;
  border-radius: 25rpx;
  display: flex;
  margin-left: auto;
  margin-right: auto;
}
.slogan{
  font-size: 25rpx;
  color: #ed5a65;
  display: block;
  text-align: center;
  justify-items: center;
}
.InputBox{
  display: block;
}
.InputBox input{
  justify-items: center;
  font-size: 28rpx;
  margin-left: auto;
  margin-right: auto;
  width: 500rpx;
  margin-top: 20rpx;
  border-bottom: 0.1px solid;
  border-bottom-color:#c4cbcf;
  height: 70rpx;
}
input{
  line-height: normal;
}
.InputBox button{
  margin-top: 35rpx;
  background-color: #CF2730;
  color: white;
  font-size: 35rpx;
  border-radius: 15rpx;
}
